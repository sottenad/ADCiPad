<h1>Model Years <%= link_to 'New Year', new_year_path, :class=>"btn btn-primary"  %></h1>

<table class="table table-hover table-bordered">
  <tr>
    <th>Year</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @years.each do |year| %>
  <tr>
    <td width="80%"><%= year.year %></td>
    <td><%= link_to 'Show', year %></td>
    <td><%= link_to 'Edit', edit_year_path(year) %></td>
    <td><%= link_to 'Destroy', year, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />


